@import "./_font-special.styl";

* {
  box-sizing: border-box;
  user-select: none;
  font-family: $font-special;
  text-align: center;
}

body {
  color: $black;
  display: flex;
  justify-content: center;
  height: 100%;
}

a {
  text-decoration: none;
  color: $black;
}

button {
  &:focus {
    outline: none;
  }
}

ul, ol {
  list-style: none;
  margin: 0;
  padding: 0;
}
